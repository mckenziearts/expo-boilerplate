{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@images/*": ["./src/images/*"],
      "@components/*": ["./src/components/*"],
      "@locales/*": ["./src/locales/*"],
      "@screens/*": ["./src/screens/*"],
      "@store/*": ["./src/store/*"],
      "@utilities/*": ["./src/utilities/*"]
    },
  }
}
